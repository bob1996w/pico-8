--shared function, switch mode

function cprint(sn,x,y,c)
	local s=""..sn
	print(s,x-4*#s/2,y,c)
end

function rprint(sn,x,y,c)
	local s=""..sn
	print(s,x-4*#s+1,y,c)
end

