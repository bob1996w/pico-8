end



--add score and convert to 32-bit int
function adds(val,mul1,mul2)
 if(mul1==nil)mul1=1
 if(mul2==nil)mul2=1
 local s=shr(val,16)
 s*=mul1
 s*=mul2
 return s
end

--get score
function gets(val)
 if(val==0)return "0"
 local s=""
 local v=abs(val)
 while (v!=0) do
  s=shl(v%0x0.000a,16)..s
  v/=10
 end
 if (val<0) s="-"..s
 return s
end

--sfx & mute
mute=false
function fx(id)
	if(not mute)sfx(id)
end
