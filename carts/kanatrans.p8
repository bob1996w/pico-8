	kana["he"]=initkach(36,5)
	kana["ho"]=initkach(39,5)
	kana["ma"]=initkach(42,5)
	kana["mi"]=initkach(45,5,2,5,0,0)
	kana["mu"]=initkach(0,10)
	kana["me"]=initkach(3,10)
	kana["mo"]=initkach(6,10)
	kana["ya"]=initkach(9,10)
	kana["yu"]=initkach(12,10)
	kana["yo"]=initkach(15,10)
	kana["ra"]=initkach(18,10)
