	printkana("xaxixuxexo xyaxyuxyoxtu        .[],-",0,12,5)
	print("this is a procedure to",0,20,7)
	print("change from roma-ji to katakana.",0,26,7)
	printkana("koreha ro-maji kara katakana he hennkannsuru",0,32,7)
	printkana("puroguramu desu.",0,38,7)
	print("the font is from m7kenji.",0,46,9)
	x,y=printkana("kono fonnto ha ",0,52,9)
	print("m7kenji",x,y,9)
	printkana("sann kara no mono desu.",0,58,9)
	print("the font takes 12 sprites.",0,65,12)
	printkana("12ko no supuraito wo siyousaremasu.",0,71,12)
	printkana("ryu",0,77,7)
end
