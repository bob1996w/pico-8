	kana["ru"]=initkach(24,10)
	kana["re"]=initkach(27,10)
	kana["ro"]=initkach(30,10)
	kana["wa"]=initkach(33,10)
	kana["wo"]=initkach(36,10)
	kana["nn"]=initkach(39,10)
	kana["xa"]=initkach(0,0,3,4,0,1)
	kana["xi"]=initkach(42,9,3,4,0,1,true)
	kana["xu"]=initkach(6,0,3,4,0,1)
