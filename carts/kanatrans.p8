	kana[","]=initkach(2,5,2,2,0,3,1)
	kana["-"]=initkach(42,9,3,2,0,2)
	kana["["]=initkach(0,12,2,3,1,0,false,true)
	kana["]"]=initkach(0,12,2,3,0,2,true,false)
end

function printk(c,px,py)
	sspr(kana[c].x,kana[c].y,kana[c].w,kana[c].h,px+kana[c].dx,py+kana[c].dy,kana[c].w,kana[c].h,kana[c].fx,kana[c].fy)
end

function printkana(str,x,y,col)
	col=col or kanacol
	pal(7,col)
	px=x -- print position
	py=y
	c=""
	for i=1,#str do
		n=sub(str,i,i)
		c=c..n
		if c=="a" or c=="i" or c=="u" or c=="e" or c=="o" or c=="." or c=="," or c=="-" or c=="[" or c=="]" then
			printk(c,px,py)
			px+=4
			c=""
		elseif c==" " then
			px+=4
			c=""
		elseif	n<"a" or n>"z" then
			print(n,px,py) px+=4
			c=""
		else
			if #c==2 then
				if n=="a" or n=="i" or n=="u" or n=="e" or n=="o" then
					p=sub(c,1,1)
					if p=="k" or p=="s" or p=="t" or p=="n" or p=="h" or p=="m" then
						printk(c,px,py)
						px+=4
						c=""
					elseif p=="y" or p=="r" or p=="w" or p=="x" then
						printk(c,px,py)
						px+=4
						c=""
					elseif p=="p" then
						c="h"..sub(c,2,2)
						printk(c,px,py)
						px+=4
						printk("d",px,py)
						px+=4
						c=""
					elseif p=="g" then
						c="k"..sub(c,2,2)
						printk(c,px,py)
						px+=4
						printk("t",px,py)
						px+=4
						c=""
					elseif p=="z" then
						c="s"..sub(c,2,2)
						printk(c,px,py)
						px+=4
						printk("t",px,py)
						px+=4
						c=""
					elseif p=="d" then
						c="t"..sub(c,2,2)
						printk(c,px,py)
						px+=4
						printk("t",px,py)
						px+=4
						c=""
					elseif p=="b" then
						c="h"..sub(c,2,2)
						printk(c,px,py)
						px+=4
						printk("t",px,py)
						px+=4
						c=""
					elseif p=="l" then
						c="x"..sub(c,2,2)
						printk(c,px,py)
						px+=4
						c=""
					else
						if(c=="fa")printk("hu",px,py)printk("xa",px+4,py)px+=8
						if(c=="fo")printk("hu",px,py)printk("xo",px+4,py)px+=8
						if(c=="ji")printk("si",px,py)printk("t",px+4,py)px+=8
						c=""
					end
				elseif c=="nn" then
					printk("nn",px,py)
					px+=4
					c=""
				elseif sub(c,1,1)=="n" then
					printk("nn",px,py)
					px+=4
					c=sub(c,2,2)
				elseif sub(c,1,1)==sub(c,2,2) then
					printk("xtu",px,py)
					px+=4
					c=sub(c,2,2)
    elseif n~="y" or n~="w" or n~="h" then
     print(sub(c,1,1),px,py)
     px+=4
     c=sub(c,2,2)
				end
			elseif #c>2 then
				f=sub(c,1,2)
				if f=="ky" or f=="sy" or f=="ty" or f=="ny" or f=="hy" or f=="hy" or f=="ry" then
					printk(sub(f,1,1).."i",px,py)
					px+=4
					if n=="a" or n=="u" or n=="o" then
						printk("xy"..n,px,py)
						px+=4
					else
						printk("x"..n,px,py)
						px+=4
					end
					c=""
				elseif f=="gy" or f=="zy" or f=="dy" then
					if f=="gy" then f="ky"
					elseif f=="zy" then f="sy"
					elseif f=="dy" then f="ty"
					end
					printk(sub(f,1,1).."i",px,py)
					px+=4
					printk("t",px,py)
					px+=4
					if n=="a" or n=="u" or n=="o" then
						printk("xy"..n,px,py)
						px+=4
					else
						printk("x"..n,px,py)
						px+=4
					end
					c=""
				elseif f=="dh" then
				--now
				
				elseif f=="xy" or f=="ly" then
					f="xy"
					printk(f..n,px,py)
					px+=4
					c=""
				elseif c=="xtu" or c=="ltu" then
					c="xtu"
					printk(c,px,py)
					px+=4
					c=""
				else
					print(c,px,py)
					px+=(#c*4)
					c=""
				end
			end
		end
	end
	pal()
	return px,py
end

function _init()
	initkana()
end

