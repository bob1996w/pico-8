	kana["su"]=initkach(36)
	kana["se"]=initkach(39)
	kana["so"]=initkach(42)
	kana["ta"]=initkach(45)
	kana["ti"]=initkach(0,5)
	kana["tu"]=initkach(3,5)
	kana["te"]=initkach(6,5)
	kana["to"]=initkach(9,5)
	kana["na"]=initkach(12,5)
	kana["ni"]=initkach(15,5)
	kana["nu"]=initkach(18,5)
	kana["ne"]=initkach(21,5)
