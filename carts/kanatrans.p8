pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--kana transform program
--by bob19996w
--kana font edited from m7kenji
kanaspr=0
kanacol=7
kana={}

function initkach(x,y,w,h,dx,dy,fx,fy)
	local k={}
	local kanasprx=(kanaspr%16)*8
	local kanaspry=flr((kanaspr/16))*8
	k["x"]=kanasprx+(x or 0)
 k["y"]=kanaspry+(y or 0)
 k["w"]=w or 3
 k["h"]=h or 5
 k["dx"]=dx or 0
 k["dy"]=dy or 0
 k["fx"]=fx -- flip hor
 k["fy"]=fy -- flip vert
 return k
end

--call in _init to setup kana
function initkana()
	kana={}
	kana["a"]=initkach()
	kana["i"]=initkach(3)
	kana["u"]=initkach(6)
	kana["e"]=initkach(9)
	kana["o"]=initkach(12)
	kana["ka"]=initkach(15)
