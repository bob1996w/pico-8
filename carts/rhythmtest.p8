pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--basic rhythm game

--song infos
title="zensekai no rhythm test"
artist="maimai"
--difficulty 1~10
songdiff={1,5,10}
--the song map
songmap={
"[52]1,1,1,1,<2>2,2,2,2,<0.5>3,3,3,3,"..
"<1>4,4,4,4,5,5,5,5,"..
"1,e",
"",
""
}

--below don't edit--
--current screen
--0=main menu
--1=playing
--2=result
screen=0
--difficulty
--0=easy
--1=normal
--2=hard
diff=0
--control
--0=zxlur���
--1=lurxz
control=0
sstart="press z to start"
score=0
combo=0
maxcombo=0
--meta: each note's frame# of perfect
meta={{},{},{},{},{}}
mt={1,1,1,1,1}
--note: each note's attributes
note={}
endmark=0
tick=0 --current tick
ttlscore=0
--whether the number is in brackets
inbrac=false
inbrac_d=1
inang=false
inang_d=1
ori_speed=1 --the original speed, if not have been change by <>
speed=1 --the parsing speed
cur=1
--frame
fr=-120

--put string in middle
function smid(s)
	return 64-((#s)*3+(#s-1))/2
end

function strtonum(s)
	num=0
	base=1
	dot=""
	for i=#s,1,-1 do
		dot=sub(s,i,i)
		if dot=="0" then num+=base*0
		elseif dot=="1" then num+=base*1
		elseif dot=="2" then num+=base*2
		elseif dot=="3" then num+=base*3
		elseif dot=="4" then num+=base*4
		elseif dot=="5" then num+=base*5
		elseif dot=="6" then num+=base*6
		elseif dot=="7" then num+=base*7
		elseif dot=="8" then num+=base*8
		elseif dot=="9" then num+=base*9
		end
		base*=10
	end
	return num
end

function analyze(d)
	while true do
	nc=sub(songmap[d],cur,cur)
	print(sub(songmap[d],cur,cur))
	if nc=="[" then
		inbrac=true
		inbrac_d=cur+1
	elseif nc=="]" then
	 	inbrac=false
		--spd=sub(songmap[d],inbrac_d,cur-1)
		ori_speed=strtonum(sub(songmap[d],inbrac_d,cur-1))
		speed=ori_speed
	elseif nc=="<" then
		inang=true
		inand_d=cur+1
	elseif nc==">" then
		inang=false
		mult=strtonum(sub(songmap[d],inand_d,cur-1))
		speed=ori_speed/mult
	elseif inbrac==false then
		if nc=="1" then
			add(meta[1],tick)
			ttlscore+=10
		elseif nc=="2" then
			add(meta[2],tick)
			ttlscore+=10
		elseif nc=="3" then
			add(meta[3],tick)
			ttlscore+=10
		elseif nc=="4" then
			add(meta[4],tick)
			ttlscore+=10
		elseif nc=="5" then
			add(meta[5],tick)
			ttlscore+=10
		elseif nc=="," then
			tick+=speed
		end
	end
	if nc=="e" then
		endmark=tick
		print("end")
		break
	end
	cur+=1
	end
end

function drawmainmenu()
	print("basic rhythm game",31,0,7)
	rect(4,12,124,38,12)
	print(title,smid(title),16,11)
	print(artist,smid(artist),29,4)
	rect(4,40,124,82,4)
	print("��select difficulty",23,44,7)
	print("easy",18,54,7)
	print("normal",18,62,7)
	print("hard",18,70,7)
	for i=0,songdiff[1]-1 do spr(0,44+i*6,54) end
	for i=0,songdiff[2]-1 do spr(1,44+i*6,62) end
	for i=0,songdiff[3]-1 do spr(2,44+i*6,70) end
	if diff==0 then spr(3,10,54)
	elseif diff==1 then spr(3,10,62)
	elseif diff==2 then spr(3,10,70)
	end
	rect(4,84,124,116,9)
	print("��controls",40,88,7)
	print("zx���       ���xz",18,100,10)
	if control==0 then spr(3,10,100)
	elseif control==1 then spr(3,70,100)
	end
	print(sstart,smid(sstart),120)
end

function drawplay()
	print(title,smid(title),2,7)
	line(0,8,128,8,11)
	line(0,120,128,120,11)
	--play line
	line(20,9,20,119,8)
	line(32,9,32,119,9)
	line(44,9,44,119,10)
	line(56,9,56,119,12)
	line(68,9,68,119,14)
	line(13,110,75,110,7)
	--display control
	if control==0 then
		print("z",19,123,8)
		print("x",31,123,9)
		print("l",41,123,10)
		print("u",53,123,12)
		print("r",65,123,14)
	elseif control==1 then
		print("l",17,123,8)
		print("u",29,123,9)
		print("r",41,123,10)
		print("x",55,123,12)
		print("z",67,123,14)
	end
end

function preview()
	--rectfill(0,0,128,5,0)
	--print(fr,0,0,7)
	if(meta[1][mt[1]]==fr)print("1",2+mt[1]*4,10,7) mt[1]+=1
	if(meta[2][mt[2]]==fr)print("2",2+mt[2]*4,15,7) mt[2]+=1
	if(meta[3][mt[3]]==fr)print("3",2+mt[3]*4,20,7) mt[3]+=1
	if(meta[4][mt[4]]==fr)print("4",2+mt[4]*4,25,7) mt[4]+=1
	if(meta[5][mt[5]]==fr)print("5",2+mt[5]*4,30,7) mt[5]+=1
	if(endmark==fr)print("end",2,50,8)
end

function _init()
	cur=0
	for df=0,2 do
	end
	s_menu()
end

function initgame()
	fr=0
end

function s_menu()
	_update60=u_menu
	_draw=d_menu
end

function s_game()
	initgame()
	_update60=u_game
	_draw=d_game
end

function u_menu()
	if(diff==0 and btnp(3))then diff=1
	elseif(diff==2 and btnp(2))then diff=1
	elseif(diff==1)then
		if(btnp(2))then diff=0
		elseif(btnp(3))then diff=2
		end
	end
	if(control==0 and btnp(1))then control=1
	elseif(control==1 and btnp(0))then control=0
	end
	if(btnp(4)) then
		analyze(diff+1)
		s_game()
	end
end

function d_menu()
	cls()
	drawmainmenu()
end

function u_game()
	fr+=1
	if(fr==1)music(0)
end

function d_game()
	cls()
	drawplay()
	preview()
end



