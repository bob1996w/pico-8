pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--"you are an idiot" emulator,
--sort of(?)
--don't worry, there are no
--virus in this cartridge.


--alt=frames per change in color
alt=20
--changes the color of default
--black and white.
col1=0
col2=7


mode=1
window={}
timer=0
in_timer=0
nt=false
initpos={30,40}
chn=0--channel

function _update()
	timer+=1
	in_timer+=1
	if(timer>alt*2)timer=0
	if mode==1 then
		if(in_timer>=20000 and nt==false)in_timer=0
		btnpress()
	end
	if mode==1 and btnp(5) then
		mode=2
		in_timer=0
		sfx(-1,0)
		--spawnwindow()
		--sfx(0,chn)
		--chn+=1
	end
	if mode==2 and btnp(5) then
		spawnwindow()
		--sfx(0,chn)
		--chn+=1
		--if(chn>=4)chn=0
		sfx(#window%4+2)
	end
end

function _init()
	cls()
	sfx(0,0)
end

function _draw()
	cls()
	if mode==1 then
		drawbig()
		if(in_timer>=300 and in_timer<=500)drawbubble()
	elseif mode==2 then
		draww()
	end
	--print(#window,0,0,7)
	drawbar()
end

function drawbig()
	if timer<=alt then
		rectfill(1,4,127,119,col1)
		pal(7,col2)
		sspr(32,0,16,16,16,72)
		sspr(32,0,16,16,56,72)
		sspr(32,0,16,16,98,72)
		print("you are an idiot",32,50,col2)
		pal()
	else
		rectfill(1,4,127,119,col2)
		pal(7,col1)
		sspr(48,0,16,16,16,72)
		sspr(48,0,16,16,56,72)
		sspr(48,0,16,16,98,72)
		print("you are an idiot",32,50,col1)
		pal()
		
	end
	rect(0,3,127,119,12)
	spr(0,0,0)
	spr(2,120,0)
	rectfill(8,0,119,7,12)
	print("you are an idiot - intern...",9,2,7)
end

function drawbar()
	spr(48,0,120)
	spr(49,8,120)
	rectfill(16,120,127,127,1)
	if mode==1 then
		rectfill(17,121,62,127,12)
		spr(16,18,121)
		print("you ar...",26,122,7)
	elseif mode==2 then
		if #window==1 then
			rectfill(17,121,62,127,12)
			spr(16,18,121)
			print("you ar...",26,122,7)
		elseif #window==2 then
			rectfill(17,121,62,127,12)
			spr(16,18,121)
			print("you ar...",26,122,7)
			rectfill(64,121,109,127,12)
			spr(16,65,121)
			print("you ar...",73,122,7)
		elseif #window==3 then
			rectfill(17,121,52,127,12)
			spr(16,18,121)
			print("you...",26,122,7)
			rectfill(54,121,89,127,12)
			spr(16,55,121)
			print("you...",63,122,7)
			rectfill(91,121,126,127,12)
			spr(16,92,121)
			print("you...",101,122,7)
		else
			rectfill(17,121,62,127,12)
			spr(16,18,121)
			print(#window,26,122,9)
			print(" you...",36,122,7)
		end
	end
end

function drawbubble()
	if(nt==false)sfx(1,2) nt=true
	rectfill(24,106,97,114,10)
	spr(17,36,114)
	print("press — to close.",26,108,0)
	
end

function btnpress()
	if btnp(2) then
		if col1==15 then col1=0
		else col1+=1 end
	end
	if btnp(3) then
		if col1==0 then col1=15
		else col1-=1 end
	end
	if btnp(1) then
		if col2==15 then col2=0
		else col2+=1 end
	end
	if btnp(0) then
		if col2==0 then col2=15
		else col2-=1 end
	end
end

function spawnwindow()
	if #window==0 then
		local w={}
		w.x=initpos[1]
		w.y=initpos[2]
		w.dirx=rnd(2)
		w.diry=rnd(2)
		w.offset=timer
		add(window,w)
		initpos[1]+=3
		initpos[2]+=3
	else
		if initpos[1]+6>90 or initpos[2]+6>90 then
			initpos[1]=30
			initpos[2]=30
		end
		local w={}
		w.x=initpos[1]
		w.y=initpos[2]
		w.dirx=rnd(2)+1
		w.diry=rnd(2)+1
		w.offset=timer
		add(window,w)
	end
end

function draww()
	for w in all(window) do
		local decide=0
		if w.offset+alt<=alt*2 then
			if timer>w.offset and timer<=w.offset+alt then
				decide=0
			else
				decide=1
			end
		else
			if timer>w.offset-alt and timer<w.offset then
				decide=1
			else
				deside=0
			end
		end
		if decide==0 then
			rectfill(w.x+1,w.y+8,w.x+31,w.y+23,col1)
			pal(7,col2)
			spr(3,w.x+2,w.y+14)
			spr(3,w.x+12,w.y+14)
			spr(3,w.x+22,w.y+14)
			print("yaai",w.x+9,w.y+9,col2)
			pal()
		else
			rectfill(w.x+1,w.y+8,w.x+31,w.y+23,col2)
			pal(7,col1)
			spr(3,w.x+2,w.y+14)
			spr(3,w.x+12,w.y+14)
			spr(3,w.x+22,w.y+14)
			print("yaai",w.x+9,w.y+9,col1)
			pal()
		end
		rect(w.x,w.y+3,w.x+31,w.y+23,12)
		spr(0,w.x,w.y)
		spr(2,w.x+24,w.y)
		rectfill(w.x+8,w.y,w.x+23,w.y+7,12)
		print("y...",w.x+8,w.y+2,7)
		
		w.x+=w.dirx
		w.y+=w.diry
		if(w.x>=95)w.dirx=-w.dirx
		if(w.x<0)w.dirx=-w.dirx
		if(w.y>=95)w.diry=-w.diry
		if(w.y<0)w.diry=-w.diry
	end
end
