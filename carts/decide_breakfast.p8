version 8
__lua__
--this cart decide what to eat
--in ncu. for breakfast.

--first entry in tables are
--area names.
border=5

shops={
	{
		"not go out",
		"don't eat"
	},
	{
		"boy dorm 9",
		"family mart",
		"jinbei hamburger",
	},
	{
		"night snack street",
		"shop#2",
		"chinese breakfast r",
		"ok mart",
		"28 brunch",
		"rei-lin mei_er_mei",
		"normal mei_er_mei"
	},
	{
		"boy dorm 7",
		"breakfast in boy dorm 7"
	},
	{
		"back door",
		"laguardia",
		"sushi",
		"hanlin big hamburger",
		"weike breakfast",
		"l.m.s"
	},
	{
		"songyuan",
		"mos burger",
		"mwd burger"
	},
	{
		"girl dorm 14",
		"laya hamburger"
	}
}

function _init()
	start_menu()
end

function initall()
	t=1
	areasel=0
	shopsel=0
	areanum=#shops
	shopnum=0
	asel=1
	ssel=1
	decided=false
	decided_countdown=100
	finald=false
end

function start_menu()
	_update60=update_menu
	_draw=draw_menu
end

function start_main()
	initall()
	_update60=update_main
	_draw=draw_main
end

function update_menu()
	if(btnp(4))start_main()
end

function draw_menu()
	cls()
	print("can't decide what to eat?",12,20,7)
	print("~ncu guide~",42,50,7)
	print("press Ž",48,80,6)
end

function update_main()
	t+=1
	if decided and decided_countdown>0 then
		decided_countdown-=1
	end
	if areasel==0 then
		asel=flr(rnd(areanum))+1
		if btnp(4) then
			areasel=asel
			sfx(0)
		end
	elseif shopsel==0 then
		ssel=flr(rnd(#shops[areasel]-1))+2
		if btnp(4) then
			shopsel=ssel
			sfx(1)
			decided=true
		end
	elseif not finald and decided_countdown <=0 then
		finald=true
		music(0)
	else
		if btnp(4) then
			initall()
		end
	end
end

function draw_main()
	cls()
	if finald then
		rectfill(0,0,127,border-1,t%16)
		rectfill(0,border-1,border-1,127-border+1,t%16)
		rectfill(0,127-border+1,127,127,t%16)
		rectfill(127-border+1,border-1,127,127)
	end
	print("area:",7,20,9)
	print(shops[asel][1],8,40,11)
	if areasel!=0 then
		print("shop:",7,80,9)
		print(shops[asel][ssel],8,100,11)
	end
end


